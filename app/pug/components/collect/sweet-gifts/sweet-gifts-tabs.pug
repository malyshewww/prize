div.collect-tabs
	.tabs__body
		.tabs__top.top-tabs
			.top-tabs__container
				ul.tabs__list
					each tabItem, tabIndex in arrayTabs
						li.tabs__item
							a.tabs__link(href=`#${tabItem.key}` class=(tabIndex==0 ? "active" : "") )= tabItem.title
		.tabs__content-wrapper.container
			each tabItem, tabIndex in arrayTabs
				div.tabs__content.tabs-content(id=tabItem.key class=(tabIndex==0 ? "show" : ""))
					form.collect__table.collect-table(id=tabItem.key=="package" ? "collectPackageForm" : "collectProdForm", class=tabItem.key=="package" ? "collect-table__package" : 0)
						.collect-table__header.collect-table__thead
							each itemHeader in tabItem.arrayHeaders
								.collect-table__th!= itemHeader
						if tabItem.key == "package"
							.collect-table__sub-header.sub-header-table
								.collect-table__tr 
									.collect-table__td.no-pacakge
										.collect-table__title Без упаковки
										.collect-table__value
										.collect-table__radiobutton.radiobutton-item
											label
												span.radiobutton-item__text 0 ₽
												input(type="radio" name="package" checked class="real-radio package-price-input")
												span.custom-radio
						.collect-table__body.collect-table__tbody
							.accordeon(data-spollers)
								each itemCategories, CategorieIndex in tabItem.objectCategories
									.accordeon__item.item-accordeon
											.item-accordeon__header.icon-arrow-next._active(data-spoller)= itemCategories.title
											.item-accordeon__content
												.item-accordeon__content-inner
													each child, childIndex in itemCategories.children
														.collect-table__row.collect-table__tr
															.collect-table__td
																.collect-table__name
																	.collect-table__images
																		.collect-table__image 
																			img(src=`img/collect/sweet-gifts/${CategorieIndex}/${childIndex+1}.png`, alt=child.title)
																			.collect-table__tooltip
																				img(src=`img/collect/sweet-gifts/${CategorieIndex}/${childIndex+1}.png`, alt=child.title)
																	h2.collect-table__title!= child.title
																.collect-table__value
																	span(data-type=" г")= child.value
																	if tabItem.key == "goods"
																		input(type="hidden" class="goods-weight-input" value=child.value)
																		input(type="hidden" class="goods-price-input" value=child.price)
																case tabItem.key
																	when "goods"
																		.collect-table__quantity.quantity-card.collect-quantity
																			button.quantity-card__button.quantity-card__button--minus.quantity-card__button--disabled(data-minus type="button")
																			input(type="text" value="0").quantity-card__input.calc-number
																			button.quantity-card__button.quantity-card__button--plus(data-plus type="button")
																	when "package"
																		.collect-table__radiobutton.radiobutton-item
																			label
																				span.radiobutton-item__text(data-type=" ₽")= child.price
																				input(type="radio" name="package" class="real-radio package-price-input" value=child.price)
																				span.custom-radio
