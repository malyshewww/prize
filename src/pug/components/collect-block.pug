- 
	collect = [
		{
			img: "collect-1",
			link: "Сладкие подарки",
			classes: "item-prize--candies-prize item-prize--big",
		},
		{
			img: "collect-2",
			link: "Орехи и сухофрукты",
			classes: "item-prize--nuts-fruits item-prize--big",
		},
	]
section.collect 
	.collect__container 
		.collect__body.prize-inner
			each item in collect
				a(href="#").collect__item.item-prize(class=item.classes)
					.item-prize__image 
						img(src=`img/collect/${item.img}.png`, alt=item.title)
					.item-prize__content
						.item-prize__text.icon-arrow-link= item.link