-
	certificates = [
		{
			img: "certificate-1",
		},
	]
section.certificate 
	.certificate__container 
		h1.certificate__title.general__title= pageTitle
	.certificate__body.container
		each item in certificates
			a(href="#").certificate__item
				.certificate__image
					img(src=`img/certificate/${item.img}.jpg`, alt="")